{
    "$schema": "https://schemas.wp.org/wp/6.0/theme.json",
    "customTemplates": [
        {
            "name": "blank",
            "postTypes": ["page", "post"],
            "title": "Blank"
        },
        {
            "name": "no-title",
            "postTypes": ["page", "post"],
            "title": "No Title"
        },
        {
            "name": "page-cover",
            "postTypes": ["page"],
            "title": "Page (Cover Image)"
        },
        {
            "name": "single-no-thumbnail",
            "postTypes": ["post"],
            "title": "Single Post (no thumbnail)"
        },
        {
            "name": "single-square-tall",
            "postTypes": ["post"],
            "title": "Single Post (tall/square image)"
        },
        {
            "name": "single-panorama",
            "postTypes": ["post"],
            "title": "Single Post (panorama image)"
        },
        {
            "name": "single-cover",
            "postTypes": ["post"],
            "title": "Single Post (Cover Image)"
        }
    ],
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "duotone": [
                {
                    "colors": ["#000", "#990099"],
                    "name": "Black and Primary",
                    "slug": "black-and-primary"
                },
                {
                    "colors": ["#000", "#0099ff"],
                    "name": "Black and Secondary",
                    "slug": "black-and-secondary"
                },
                {
                    "colors": ["#000", "#00cccc"],
                    "name": "Black and Tertiary",
                    "slug": "black-and-tertiary"
                }
            ],
            "gradients": [
                {
                    "gradient": "linear-gradient(135deg,rgba(0,0,0,1) 50%,var(--wp--preset--color--primary) 100%)",
                    "name": "Black to Primary",
                    "slug": "black-primary"
                },
                {
                    "gradient": "linear-gradient(135deg,rgba(0,0,0,1) 50%,var(--wp--preset--color--secondary) 100%)",
                    "name": "Black to Secondary",
                    "slug": "black-secondary"
                },
                {
                    "gradient": "linear-gradient(135deg,rgba(0,0,0,1) 50%,var(--wp--preset--color--tertiary) 100%)",
                    "name": "Black to Tertiary",
                    "slug": "black-tertiary"
                }
            ],
            "palette": [
                {
                    "color": "#990099",
                    "name": "Primary",
                    "slug": "primary"
                },
                {
                    "color": "#0099ff",
                    "name": "Secondary",
                    "slug": "secondary"
                },
                {
                    "color": "#00cccc",
                    "name": "Tertiary",
                    "slug": "tertiary"
                },
                {
                    "color": "#000",
                    "name": "Main",
                    "slug": "main"
                },
                {
                    "color": "#fff",
                    "name": "Base",
                    "slug": "base"
                }
            ]
        },
        "custom": {
            "fontWeight": {
                "black": 900,
                "bold": 700,
                "extra-bold": 800,
                "light": 300,
                "medium": 500,
                "regular": 400,
                "semi-bold": 600
            },
            "lineHeight": {
                "body": 1.5,
                "heading": 1.2
            },
            "spacing": {
                "outer": "30px"
            }
        },
        "layout": {
            "contentSize": "720px",
            "wideSize": "1200px"
        },
        "spacing": {
            "units": ["px", "em", "rem", "vh", "vw", "%"]
        },
        "typography": {
            "dropCap": false,
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": ["file:./assets/fonts/Barlow-Regular.woff2"]
                        },
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "300",
                            "src": ["file:./assets/fonts/Barlow-Light.woff2"]
                        },
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "400",
                            "src": ["file:./assets/fonts/Barlow-Italic.woff2"]
                        },
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "900",
                            "src": ["file:./assets/fonts/Barlow-Black.woff2"]
                        },
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "500",
                            "src": ["file:./assets/fonts/Barlow-Medium.woff2"]
                        },
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "500",
                            "src": ["file:./assets/fonts/Barlow-Italic.woff2"]
                        },
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "600",
                            "src": ["file:./assets/fonts/Barlow-SemiBold.woff2"]
                        },
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": ["file:./assets/fonts/Barlow-Bold.woff2"]
                        },
                        {
                            "fontFamily": "Barlow",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "800",
                            "src": [
                                "file:./assets/fonts/Barlow-ExtraBold.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "Barlow, sans-serif",
                    "name": "Barlow",
                    "slug": "primary"
                },
                {
                    "fontFamily": "monospace",
                    "name": "Monospace",
                    "slug": "monospace"
                }
            ],
            "fontSizes": [
                {
                    "name": "xSmall",
                    "size": "14px",
                    "slug": "xsmall"
                },
                {
                    "name": "Small",
                    "size": "16px",
                    "slug": "small"
                },
                {
                    "name": "Medium",
                    "size": "20px",
                    "slug": "medium"
                },
                {
                    "name": "Large",
                    "size": "24px",
                    "slug": "large"
                },
                {
                    "name": "xLarge",
                    "size": "30px",
                    "slug": "x-large"
                },
                {
                    "name": "24px",
                    "size": "clamp(18px, 3vw, 24px)",
                    "slug": "max-24"
                },
                {
                    "name": "19px",
                    "size": "clamp(16px, 3vw, 19px)",
                    "slug": "max-19"
                },
                {
                    "name": "30px",
                    "size": "clamp(20px, 3vw, 30px)",
                    "slug": "max-30"
                },
                {
                    "name": "36px",
                    "size": "clamp(24px, 3vw, 36px)",
                    "slug": "max-36"
                },
                {
                    "name": "48px",
                    "size": "clamp(30px, 4vw, 48px)",
                    "slug": "max-48"
                },
                {
                    "name": "60px",
                    "size": "clamp(36px, 5vw, 60px)",
                    "slug": "max-60"
                },
                {
                    "name": "72px",
                    "size": "clamp(48px, 6vw, 72px)",
                    "slug": "max-72"
                }
            ]
        }
    },
    "styles": {
        "blocks": {
            "core/calendar": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/code": {
                "border": {
                    "radius": "0"
                },
                "color": {
                    "background": "var(--wp--preset--color--main)",
                    "text": "var(--wp--preset--color--base)"
                },
                "spacing": {
                    "padding": {
                        "bottom": "25px",
                        "left": "30px",
                        "right": "30px",
                        "top": "25px"
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)",
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/columns": {
                "spacing": {
                    "margin": {
                        "bottom": "0px"
                    }
                }
            },
            "core/comment-author-name": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "var(--wp--custom--font-weight--semi-bold)",
                    "lineHeight": "var(--wp--custom--line-height--heading)"
                }
            },
            "core/comment-date": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xsmall)"
                }
            },
            "core/comment-edit-link": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xsmall)"
                }
            },
            "core/comment-reply-link": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/cover": {
                "spacing": {
                    "padding": {
                        "bottom": "30px",
                        "left": "30px",
                        "right": "30px",
                        "top": "30px"
                    }
                }
            },
            "core/group": {
                "spacing": {
                    "padding": "0px"
                }
            },
            "core/heading": {
                "spacing": {
                    "margin": {
                        "bottom": "10px",
                        "top": "0px"
                    }
                },
                "typography": {
                    "lineHeight": "var(--wp--custom--line-height--heading)"
                }
            },
            "core/image": {
                "spacing": {
                    "margin": {
                        "bottom": "30px",
                        "left": "0",
                        "right": "0",
                        "top": "0"
                    }
                }
            },
            "core/navigation": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/post-content": {
                "spacing": {
                    "margin": {
                        "bottom": "0",
                        "left": "0",
                        "right": "0",
                        "top": "0"
                    }
                }
            },
            "core/preformatted": {
                "border": {
                    "color": "var(--wp--preset--color--main)",
                    "radius": "0"
                },
                "color": {
                    "background": "var(--wp--preset--color--main)",
                    "text": "var(--wp--preset--color--base)"
                },
                "spacing": {
                    "padding": {
                        "bottom": "25px",
                        "left": "30px",
                        "right": "30px",
                        "top": "25px"
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)",
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/pullquote": {
                "border": {
                    "color": "var(--wp--preset--color--main)",
                    "style": "solid",
                    "width": "1px"
                },
                "spacing": {
                    "padding": {
                        "bottom": "30px",
                        "left": "30px",
                        "right": "30px",
                        "top": "30px"
                    }
                }
            },
            "core/query-title": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--max-48)"
                }
            },
            "core/quote": {
                "border": {
                    "color": "var(--wp--preset--color--main)",
                    "style": "solid !important",
                    "width": "1px !important"
                },
                "spacing": {
                    "margin": {
                        "bottom": "35px"
                    },
                    "padding": {
                        "bottom": "30px",
                        "left": "40px !important",
                        "right": "40px !important",
                        "top": "30px"
                    }
                }
            },
            "core/separator": {
                "color": {
                    "text": "var(--wp--preset--color--main)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "core/site-tagline": {
                "spacing": {
                    "margin": {
                        "bottom": "20px"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/site-title": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "core/spacer": {
                "spacing": {
                    "margin": {
                        "top": "0 !important"
                    }
                }
            },
            "core/table": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/template-part": {
                "spacing": {
                    "margin": {
                        "top": "0px"
                    }
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--base)",
            "text": "var(--wp--preset--color--main)"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--max-36)",
                    "fontWeight": "var(--wp--custom--font-weight--extra-bold)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--max-24)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--main)"
                }
            }
        },
        "spacing": {
            "blockGap": "30px"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--primary)",
            "fontSize": "var(--wp--preset--font-size--max-19)",
            "fontWeight": "var(--wp--custom--font-weight--regular)",
            "lineHeight": "var(--wp--custom--line-height--body)"
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "Header"
        },
        {
            "area": "footer",
            "name": "footer",
            "title": "Footer"
        }
    ],
    "version": 2
}
